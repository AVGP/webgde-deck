{
  "name": "chromestatus-deck",
  "version": "0.0.1",
  "scripts": {
    "clean": "rm -rf dist/",
    "prestart": "node ./tools/generatemanifest.js",
    "build:server": "babel src -d dist/server",
    "build:client": "rollup -c",
    "build:assets": "mkdir -p dist/server/public/assets/ && mkdir -p dist/server/public/data && cp -R src/public/assets/* dist/server/public/assets/ && cp src/public/data/* dist/server/public/data/",
    "build": "npm run build:server && npm run build:client && npm run build:assets",
    "start": "node ./tools/generatemanifest.js && node ./dist/server.js"
  },
  "dependencies": {
    "express": "^4.15.2",
    "node-fetch": "*",
    "text-encoding": "*",
    "xmldom-alpha": "*",
    "workbox-build": "^1.3.0",
    "workbox-sw": "^1.3.0"
  },
  "engines": {
    "node": "6.9.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/paulkinlan/web-dev-deck.git"
  },
  "license": "Apache-2.0",
  "keywords": [
    "node",
    "glitch",
    "express"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1"
  }
}
